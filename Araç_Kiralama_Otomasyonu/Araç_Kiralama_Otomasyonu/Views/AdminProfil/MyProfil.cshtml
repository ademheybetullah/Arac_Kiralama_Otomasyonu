
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Scripts/Css/ViewProfileForAdmin.css" rel="stylesheet" />
    <link href="~/Scripts/Css/Navbar.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
    <title>MyProfil</title>
    <style>
        .Degisken{
            overflow:hidden;
            height:50px;
        }
    </style>
</head>
<body>
    @Html.Partial("AdminNavbar")
    <div style="height: auto; padding-bottom: 20px;" id="Kutu">
        <div id="AdminIconDiv">
            <img src="~/Scripts/Icons/Admin.png" id="AdminIcon">
        </div>
        <div class="IconDiv">
            <img src="~/Scripts/Icons/Id.png" id="IdIcon">
        </div>
        <div class="Degisken">
            @Session["Id"]
        </div>
        <div class="IconDiv">
            <img src="~/Scripts/Icons/Ad.png" id="AdIcon">
        </div>
        <div class="Degisken">
            @Session["Name"]
        </div>
        <div class="IconDiv">
            <img src="~/Scripts/Icons/Soyad.png" id="SoyadIcon">
        </div>
        <div class="Degisken">
            @Session["Surname"]
        </div>
        <div class="IconDiv">
            <img src="~/Scripts/Icons/Eposta.png" id="EpostaIcon">
        </div>
        <div class="Degisken">
            @Session["Email"]
        </div>

    </div>
    @Html.Partial("_Footer")
</body>
</html>
